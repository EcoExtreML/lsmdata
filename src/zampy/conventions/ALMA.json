{
    "ALMA": {
        "latitude_name": "x",
        "longitude_name": "y",
        "latitude_format": "(-90,90)",
        "longitude_format": "(-180,180)"
    },
    "height_of_vegetation":{
        "variable": "Hveg",
        "units": "meter"
    },
    "height_of_vegetation_standard_deviation":{
        "variable": "Hveg_sd",
        "units": "meter"
    },    
    "net_shortwave_radiation": {
        "variable": "SWnet",
        "units": "watt/meter**2",
        "positive_dir": "downward"
    },
    "net_longwave_radiation": {
        "variable": "LWnet",
        "units": "watt/meter**2",
        "positive_dir": "downward"
    },
    "latent_heat_flux": {
        "variable": "Qle",
        "units": "watt/meter**2",
        "positive_dir": "downward"
    },
    "sensible_heat_flux": {
        "variable": "Qh",
        "units": "watt/meter**2",
        "positive_dir": "downward"
    },
    "surface_pressure": {
        "variable": "Psurf",
        "units": "pascal"
    },
    "10m_u_component_of_wind": {
        "variable": "Wind_N",
        "units": "meter/second"
    },
    "10m_v_component_of_wind": {
        "variable": "Wind_E",
        "units": "meter/second"
    },
    "mean_total_precipitation_rate": {
        "variable": "Rainf",
        "units": "kilogram/(meter**2*second)"
    }
}